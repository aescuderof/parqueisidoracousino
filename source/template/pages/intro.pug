extends ../layouts/primary.pug

block variables

block header

	// menu dropdown start

	.menu-dropdown

		.menu-dropdown__inner(data-value="start")

			.screen.screen--start

				ul.aside-menu

					li.main-menu__item
						a.main-menu__link.main-menu__link--scroll(href="#front-pages")
							span Demo

					li.main-menu__item
						a.main-menu__link.main-menu__link--scroll(href="#inner-pages")
							span Inner Pages

					li.main-menu__item
						a.main-menu__link.main-menu__link--scroll(href="#features")
							span Features

					li.main-menu__item
						a.main-menu__link.main-menu__link--scroll(href="#technologies")
							span Used Technologies

				a.screen__button(href="#") Buy Wild World

	// menu dropdown end

	// header start

	header.header.header-intro
		.row.justify-content-between.align-items-center

			.col-auto.d-flex.align-items-center

				.header-logo
					a.header-logo__link(href="index.html")
						img.logo__img.logo--light(src="img/logo_zoo_white.png", alt="logo")
						img.logo__img.logo--dark(src="img/logo_zoo_black.png", alt="logo")

			.col-auto

				// main menu start

				nav
					ul.main-menu

						li.main-menu__item
							a.main-menu__link.main-menu__link--scroll(href="#front-pages")
								span Demo

						li.main-menu__item
							a.main-menu__link.main-menu__link--scroll(href="#inner-pages")
								span Inner Pages

						li.main-menu__item
							a.main-menu__link.main-menu__link--scroll(href="#features")
								span Features

						li.main-menu__item
							a.main-menu__link.main-menu__link--scroll(href="#technologies")
								span Used Technologies

				// main menu end

			.col-auto.d-flex.align-items-center

				a.button.button--filled(href="#" target="_blank")
					span Buy Wild World

				.hamburger.d-block.d-lg-none
					.hamburger-box
						.hamburger-inner

	// header end

block main

	// intro start

	.intro-promo
		img.img--bg(src="img/intro_background.png" alt="promo")
		img.intro-promo__img(src="img/intro_promo.png", alt="img")

		.container
			.row

				.col-lg-7.col-xl-6
					.align-container
						.align-container__item
							h6.intro-promo__pre-title Wild World
							h2.intro-promo__title Zoo | Safari #[br] & Animal Protection #[br]#[span HTML Template]
							p.intro-promo__subtitle Animal World is creative template for zoo and safari business. It can provides your awesome business with creative design and great functionality.
							a(href="#front-pages").button.intro-promo__button.button--primary.main-menu__link--scroll See demos

		a.promo__anchor.main-menu__link--scroll(href="#front-pages")
			span Scroll Down

	// intro end

	// section start

	section.section.front-demos#front-pages
		img.front-demos__bg(src="img/demos_img.png" alt="img")
		.front-demos__container

			.row.bottom-50
				.col-12

					.heading.heading--primary.heading--center
						span.heading__pre-title pre-built DEmos
						h2.heading__title.no-margin-bottom #[span Awesome 6] #[span home pages]

			.row.offset-70

				.col-sm-6.col-md-4

					.demo-item
						.demo-item__img
							img.img--contain(src="img/front_1.jpg" alt="front")
							a.demo-item__link.button.button--primary(href="index.html" target="_blank") See Page
						h6.demo-item__title 01. Home page “ZOO”

				.col-sm-6.col-md-4

					.demo-item
						.demo-item__img
							img.img--contain(src="img/front_2.jpg" alt="front")
							a.demo-item__link.button.button--primary(href="front_2.html" target="_blank") See Page
						h6.demo-item__title 02. Home page “Aquarium”

				.col-sm-6.col-md-4

					.demo-item
						.demo-item__img
							img.img--contain(src="img/front_3.jpg" alt="front")
							a.demo-item__link.button.button--primary(href="front_3.html" target="_blank") See Page
						h6.demo-item__title 03. Home page “Terrarium”

				.col-sm-6.col-md-4

					.demo-item
						.demo-item__img
							img.img--contain(src="img/front_4.jpg" alt="front")
							a.demo-item__link.button.button--primary(href="front_5.html" target="_blank") See Page
						h6.demo-item__title 04. Home page “Safari”

				.col-sm-6.col-md-4

					.demo-item
						.demo-item__img
							img.img--contain(src="img/front_5.jpg" alt="front")
							a.demo-item__link.button.button--primary(href="front_4.html" target="_blank") See Page
						h6.demo-item__title 05. Home page “Animal protection”

				.col-sm-6.col-md-4

					.demo-item
						.demo-item__img
							img.img--contain(src="img/front_6.jpg" alt="front")
							a.demo-item__link.button.button--primary(href="front_6.html" target="_blank") See Page
						h6.demo-item__title 06. Home page “Shop”

	// section end

	// section start

	section.section.inner-pages#inner-pages.no-padding-bottom

		img.img--bg(src="img/inner-pages__bg.jpg", alt="img")

		.container
			.row.bottom-50.align-items-end

				.col-lg-6.col-xl-5

					.heading.heading--primary
						span.heading__pre-title Inner Pages
						h2.heading__title.color--white #[span Great] #[span Inner pages]
						p.no-margin-bottom.color--white We created 50 pre-built pages for your purposes and e-commerce pages included to template

				.col-lg-6.col-xl-7.text-lg-right

					.pages-slider__dots

		.slider-holder.offset-70

			.slider-holder__wrapper

				.pages-slider

					.pages-slider__item

						.demo-item
							.demo-item__img
								img.img--contain(src="img/d1.jpg" alt="page")
								a.demo-item__link.button.button--primary(href="about.html" target="_blank") See Page
							h6.demo-item__title 01. About Zoo

					.pages-slider__item

						.demo-item
							.demo-item__img
								img.img--contain(src="img/d2.jpg" alt="page")
								a.demo-item__link.button.button--primary(href="gallery.html" target="_blank") See Page
							h6.demo-item__title 02. Gallery

					.pages-slider__item

						.demo-item
							.demo-item__img
								img.img--contain(src="img/d3.jpg" alt="page")
								a.demo-item__link.button.button--primary(href="membership.html" target="_blank") See Page
							h6.demo-item__title 03. Membership

					.pages-slider__item

						.demo-item
							.demo-item__img
								img.img--contain(src="img/d4.jpg" alt="page")
								a.demo-item__link.button.button--primary(href="foundation.html" target="_blank") See Page
							h6.demo-item__title 04. Foundation

					.pages-slider__item

						.demo-item
							.demo-item__img
								img.img--contain(src="img/d5.jpg" alt="page")
								a.demo-item__link.button.button--primary(href="animals.html" target="_blank") See Page
							h6.demo-item__title 05. Animals

					.pages-slider__item

						.demo-item
							.demo-item__img
								img.img--contain(src="img/d7.jpg" alt="page")
								a.demo-item__link.button.button--primary(href="services.html" target="_blank") See Page
							h6.demo-item__title 06. Services

	// section end

	// section start

	section.section.intro-features#features.no-padding-bottom

		img.section__bg.t0.r0(src="img/features_1.png", alt="img")
		img.section__bg.t50.l0(src="img/features_2.png", alt="img")

		section.no-padding-top.section
			.container
				.row

					.col-lg-6

						.features-item__img
							img.img--containe(src="img/intro-feat_1.png", alt="img")

					.col-lg-6

						.heading.heading--style-2.bottom-20
							span.heading__pre-title features
							h2.heading__title.no-margin-bottom #[span pre-built shop pages for] #[span your e-commerce]

						p.bottom-30 We created and included inner pages for e-commerce. You can use it for different purposes, as example for sale and rent your product online.

						a.button.button--primary(href="shop-catalog.html" target="_blank") See all pages

		section.section.no-padding-top

			.container
				.row.flex-column-reverse.flex-lg-row

					.col-lg-6

						.heading.heading--style-2.bottom-20
							span.heading__pre-title features
							h2.heading__title.no-margin-bottom #[span collection of over 100] #[span different icons]

						p.bottom-30 We collected for you +100 different modern icons that we`ll be great for your bussiness. You can use it for your website and you don`t need icons on other websites.

						a.button.button--primary(href="icon-list.html" target="_blank") See all Icons

					.col-lg-6

						.features-item__img
							img.img--containe(src="img/intro-feat_2.png", alt="img")

		section.section.no-padding-top.no-padding-bottom
			.container

				.row

					.col-lg-6

						.features-item__img
							img.img--containe(src="img/intro-feat_3.png", alt="img")

					.col-lg-6

						.heading.heading--style-2.bottom-20
							span.heading__pre-title features
							h2.heading__title.no-margin-bottom #[span more than 100 elements] #[span you can customise]

						p.bottom-30 This website includes 100 + elements. So you can easily customise it for building different creative pages and blocks, and make a truly unique site.

						a.button.button--primary(href="testimonials.html" target="_blank") See all pages

	// section end

	// section start

	section.section#technologies
		.container
			.row.bottom-50
				.col-12

					.heading.heading--primary.heading--center
						span.heading__pre-title Technology
						h2.heading__title.no-margin-bottom #[span Used Technologies]

			.row.offset-margin

				.col-sm-6.col-lg-3

					a.technologie-item(href="https://getbootstrap.com/" target="_blank")
						.technologie-item__img
							svg.icon.technologie-item__icon(style="font-size: 160px;")
								use(xlink:href="#bootstrap")

				.col-sm-6.col-lg-3

					a.technologie-item(href="https://gulpjs.com/" target="_blank")
						.technologie-item__img
							svg.icon.technologie-item__icon(style="font-size: 90px;")
								use(xlink:href="#gulp")

				.col-sm-6.col-lg-3

					a.technologie-item(href="https://jquery.com/" target="_blank")
						.technologie-item__img
							svg.icon.technologie-item__icon(style="font-size: 130px;")
								use(xlink:href="#jquery")

				.col-sm-6.col-lg-3

					a.technologie-item(href="https://nodejs.org/" target="_blank")
						.technologie-item__img
							svg.icon.technologie-item__icon(style="font-size: 130px;")
								use(xlink:href="#node")

	// section end

block footer

	// footer start

	section.section.intro-footer
		img.img--bg(src="img/intro-footer.jpg" alt="img")

		.container
			.row

				.col-12

					.heading.heading--primary.text-center
						span.heading__pre-title.color--golden You can buy
						h2.heading__title.color--white.no-margin-bottom #[span Purchase zoo | safari & animal protection] #[br] #[span HTML Template]
						a.button.intro-footer__button.button--primary(href="#" target="_blank") Purchase

	// footer end